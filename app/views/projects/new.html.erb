<div class='container'>
	<div class="title center">
		New Project Registration</div>

	<%= form_for @project do |f| %>
		<%= f.label :title, class: 'label'%>
		<%= f.text_field :title %>

		<%= f.label :description, class: 'label' %>
		<%= f.text_area :description %>

		<%= f.submit 'Submit Project', class: 'btn btn-submit' %>
	<% end %>
</div>